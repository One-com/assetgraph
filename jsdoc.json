{
  "opts": {
    "template": "node_modules/docdash",
    "encoding": "utf8",
    "destination": "./jsdoc/",
    "recurse": true,
    "readme": "README.md"
  },
  "source": {
    "include": "lib",
    "includePattern": ".js$"
  },
  "plugins": [
    "plugins/markdown",
    "jsdoc-plugins/remove-imports",
    "jsdoc-plugins/underscore-private",
    "jsdoc-plugin-intersection",
    "jsdoc-inheritance-diagram"
  ],
  "docdash": {
    "static": true,
    "sort": true
  }
}
