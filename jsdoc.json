{
  "opts": {
    "template": "node_modules/docdash",
    "encoding": "utf8",
    "destination": "./jsdoc/",
    "recurse": true,
    "readme": "README.md"
  },

  "source": {
    "include": "lib",
    "includePattern": ".js$"
  },

  "plugins": [
      "plugins/markdown"
  ],

  "docdash": {
      "static": true,
      "sort": true,
      "search": true,
      "collapse": true,
      "typedefs": true,
    
      "menu": {
          "Github Repository": {
              "href": "https://github.com/assetgraph/assetgraph",
              "target":"_blank",
              "class": "menu-item",
              "id": "github_link"
          },
          "Gitter Chat": {
              "href": "https://gitter.im/assetgraph/assetgraph",
              "target": "_blank",
              "class": "menu-item",
              "id": "gitter_chat_link"
          }
      }
  }
}
